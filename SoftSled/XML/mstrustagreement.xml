            <direction>out</direction>
            <relatedStateVariable>A_ARG_TYPE_Authenticator</relatedStateVariable>
         </argument>
        </argumentList>
      </action>
      <action>
        <name>Validate</name>
        <argumentList>
          <argument>
            <name>HostID</name>
            <direction>in</direction>
            <relatedStateVariable>A_ARG_TYPE_EndpointID</relatedStateVariable>
